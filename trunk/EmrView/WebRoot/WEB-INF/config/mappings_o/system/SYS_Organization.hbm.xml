<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SYS_Organization" table="sys_organization">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="sequence">
            		<param name="sequence">SEQ_SYS_Organization</param>
            </generator>
		</id>
		<property name="organizCode" type="java.lang.String">
			<column name="organizCode" length="50" />
		</property>
		<property name="organizName" type="java.lang.String">
			<column name="organizName" length="50"></column>
		</property>
		<property name="organizType" type="java.lang.String">
			<column name="organizType" length="50" />
		</property>
		<property name="classifyCode" type="java.lang.String">
			<column name="classifyCode" length="50" />
		</property>
		<property name="adminDivision" type="java.lang.String">
			<column name="adminDivision" length="50" />
		</property>
		<property name="streeName" type="java.lang.String">
			<column name="streeName" length="50" />
		</property>
		<property name="streeCode" type="java.lang.String">
			<column name="streeCode" length="50" />
		</property>
		<property name="hostCode" type="java.lang.String">
			<column name="hostCode" length="50" />
		</property>
		<property name="subCode" type="java.lang.String">
			<column name="subCode" length="50" />
		</property>
		<property name="parentId" type="java.lang.String">
			<column name="parentId" length="50" />
		</property>
		<property name="address" type="java.lang.String">
			<column name="address" length="100" />
		</property>
		<property name="zipCode" type="java.lang.String">
			<column name="zipCode" length="50" />
		</property>
		<property name="telphone" type="java.lang.String">
			<column name="telphone" length="50" />
		</property>
		<property name="email" type="java.lang.String">
			<column name="email" length="50" />
		</property>
		<property name="website" type="java.lang.String">
			<column name="website" length="50" />
		</property>
		<property name="foundDate" type="date">
			<column name="foundDate" length="10" />
		</property>
		<property name="legal" type="java.lang.String">
			<column name="legal" length="50" />
		</property>
		<property name="director" type="java.lang.String">
			<column name="director" length="50" />
		</property>
		<property name="subNum" type="java.lang.Integer">
			<column name="subNum" />
		</property>
		<property name="stationNum" type="java.lang.Integer">
			<column name="stationNum" />
		</property>
		<property name="grade" type="java.lang.String">
			<column name="grade" length="10"/>
		</property>
		<property name="institLevel" type="java.lang.String">
			<column name="institLevel" length="10"/>
		</property>
		<property name="buildingArea" type="java.lang.String">
			<column name="buildingArea" length="10"/>
		</property>
		<property name="chemicalMedNum" type="java.lang.String">
			<column name="chemicalMedNum" length="10"/>
		</property>
		<property name="chineseMedNum" type="java.lang.String">
			<column name="chineseMedNum" length="10"/>
		</property>
		<property name="pyCode" type="java.lang.String">
			<column name="pyCode" />
		</property>
	</class>
</hibernate-mapping>
