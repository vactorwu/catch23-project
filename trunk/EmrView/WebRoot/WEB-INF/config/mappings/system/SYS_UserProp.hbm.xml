<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SYS_UserProp" table="SYS_UserProp">
		<id name="id" type="java.lang.Integer" length="30">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="userId" type="java.lang.String" length="30" />
		<property name="jobId" type="java.lang.String" length="20" />
		<property name="manaUnitId" type="java.lang.String" length="50" />
		<property name="post" type="java.lang.String" length="50" />
		<property name="regionCode" type="java.lang.String" length="50" />
		<property name="domain" type="java.lang.String" length="8" />
	</class>
</hibernate-mapping>