<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="Mon_Service" table="Mon_Service">
		<id name="id" type="java.lang.Integer">
	    	<column name="id"/>
			<generator class="identity" />
		</id>
		<property name="serviceName" type="java.lang.String" length="100" />
		<property name="execCount" type="java.lang.Integer"  />
		<property name="maxTimecost" type="java.lang.Integer"  />
		<property name="lastExecTime" type="timestamp"  />
		<property name="lastReportTime" type="timestamp"  />
		<property name="domainId" type="java.lang.String" length="50" />
		<property name="ipAddress" type="java.lang.String" length="30" />
	</class>
</hibernate-mapping>